<GridPane alignment="center" hgap="10" minHeight="750.0" minWidth="900.0" prefHeight="750.0" prefWidth="900.0" styleClass="root" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs351.core.Game">
  <padding><Insets bottom="10" left="25" right="25" top="25" /></padding>
   <children>
     
     <Text id="welcome-text" fx:id="gameTitle" text="Zombie House" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
    <HBox alignment="bottom_right" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="9" />
      <ToolBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="7">
        <items>
               <Button fx:id="playButton" onAction="#handlePlay" prefWidth="60" text="Pause" />
               <Button onAction="#handleClear" prefWidth="60" text="Reset" />
            <Button onAction="#handleSubmitButtonAction" prefWidth="60" text="Quit" />
            <Label alignment="CENTER_RIGHT" prefHeight="15.0" prefWidth="74.0" text="" />
            <Text id="stamina"  fx:id="stamina" text="" />
        </items>
      </ToolBar>
      <StackPane prefHeight="542.0" prefWidth="846.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
      
   </children>
  
  <stylesheets>
    <URL value="@Login.css" />
  </stylesheets>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
  
</GridPane>
